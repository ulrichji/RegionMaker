<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>

<AnchorPane xmlns:fx="http://javafx.com/fxml">
	<GridPane hgap="10" vgap="10">
		<Label text="Sea Level:" GridPane.columnIndex="0" GridPane.rowIndex="0"/>
		<TextField fx:id="tf_seaLevel" GridPane.columnIndex="1" GridPane.rowIndex="0"/>
		<Label text="Ocean angle:" GridPane.columnIndex="0" GridPane.rowIndex="1"/>
		<TextField fx:id="tf_oceanAngle" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
	</GridPane>
</AnchorPane>

