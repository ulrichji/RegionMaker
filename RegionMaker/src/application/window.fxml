<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>


<VBox xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <MenuBar prefHeight="24.0" prefWidth="600.0">
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
                  <MenuItem mnemonicParsing="false" onAction="#openFile" text="Open .dem file" />
              <MenuItem mnemonicParsing="false" text="Close" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Edit">
               <items>
                  <Menu mnemonicParsing="false" text="Game">
                    <items>
                      <MenuItem mnemonicParsing="false" text="Cities: Skylines" />
                        <MenuItem mnemonicParsing="false" text="Sim City 4" />
                    </items>
                  </Menu>
               </items>
          </Menu>
        </menus>
      </MenuBar>
      <ScrollPane prefHeight="200.0" prefWidth="200.0">
         <content>
            <ImageView pickOnBounds="true" preserveRatio="true" />
         </content>
      </ScrollPane>
   </children>
</VBox>
